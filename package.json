{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "preview": "nuxt preview",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrate": "wrangler d1 migrations apply DB --local",
    "db:migrate:test": "wrangler d1 migrations apply DB --remote --env=\"\"",
    "db:migrate:prod": "wrangler d1 migrations apply DB --remote --env=\"production\"",
    "db:reset": "rimraf .wrangler/state/v3/d1/miniflare-D1DatabaseObject && pnpm run db:migrate",
    "db:backup": "wrangler d1 export DB --output=backup-local.sql --local",
    "db:backup:test": "wrangler d1 export DB --output=backup-test.sql --remote --env=\"\"",
    "db:backup:prod": "wrangler d1 export DB --output=backup-prod.sql --remote --env=\"production\"",
    "deploy:test": "pnpm run build && wrangler deploy --assets .output/public --env=\"\"",
    "deploy:prod": "pnpm run build && wrangler deploy --assets .output/public --env=\"production\"",
    "deploy:all": "pnpm run deploy:test && pnpm run deploy:prod"
  },
  "dependencies": {
    "cloudflare-workers-and-google-oauth": "^2.0.0",
    "cookie": "^1.0.2",
    "daisyui": "^5.0.50",
    "drizzle-orm": "^0.44.4",
    "jsonwebtoken": "^9.0.2",
    "nuxt": "^4.0.1",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.7.1",
    "@pinia/nuxt": "^0.11.2",
    "@tailwindcss/vite": "^4.1.11",
    "@types/jsonwebtoken": "^9.0.10",
    "eslint": "^9.32.0",
    "h3": "^1.15.4",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.6",
    "wrangler": "^4.32.0"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
