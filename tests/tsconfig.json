{
  "extends": "../.nuxt/tsconfig.server.json",
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    "./**/*.vue",
    "./globals.d.ts"
  ],
  "exclude": [
    "../node_modules"
  ],
  "compilerOptions": {
    "types": ["vitest/globals"],
    "skipLibCheck": true,
    "paths": {
       "~/*": ["../app/*"],
       "@/*": ["../app/*"],
      "~~/*": ["../*"],
       "@@/*": ["../*"],
       "#fixtures/*": ["./fixtures/*"]
     }
  }
}
